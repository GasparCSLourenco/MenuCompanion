<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserPage"
    android:background="@color/brandBG">

    <ImageView
        android:id="@+id/imageViewHistory"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/history"
        app:layout_constraintStart_toEndOf="@+id/imageViewOrder"
        app:layout_constraintTop_toTopOf="@+id/imageViewOrder"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView15"
        style="@style/BrandText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="View History"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageViewHistory"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/imageViewHistory"
        app:layout_constraintTop_toBottomOf="@+id/imageViewHistory" />

    <TextView
        android:id="@+id/textView9"
        style="@style/BrandText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="View Menu"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="@+id/imageViewOrder"
        app:layout_constraintHorizontal_bias="0.518"
        app:layout_constraintStart_toStartOf="@+id/imageViewOrder"
        app:layout_constraintTop_toBottomOf="@+id/imageViewOrder" />

    <ImageView
        android:id="@+id/imageViewOrder"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="124dp"
        android:src="@drawable/menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.199"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewWelcome"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textViewWelcome"
        style="@style/BrandTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Welcome, "
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonQuit"
        style="@style/BrandButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewOrder" />

</androidx.constraintlayout.widget.ConstraintLayout>